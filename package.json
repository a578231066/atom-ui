{
    "name": "vue-atom-ui",
    "version": "1.0.155-beta.1",
    "description": "vue-atom-ui",
    "author": "宁宁 <383514580@qq.com>",
    "main": "dist/atom.min.js",
    "scripts": {
        "dev": "node build/dev-server.js",
        "start": "npm run dev",
        "pub": "rm -rf dist/ && gulp build  && node build/publish.js",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "test": "npm run unit",
        "clean:lib": "rimraf lib --yes",
        "build": "node build/build.js",
        "build:utils": "babel src/utils --out-dir lib/utils",
        "build:mixin": "babel src/mixin --out-dir lib/mixin",
        "build:locale": "babel src/locale --out-dir lib/locale",
        "build:directives": "babel src/directives --out-dir lib/directives",
        "build:scss": "gulp build --gulpfile build/gen-scss.js",
        "build:packages": "npm run clean:lib && cooking build -c build/cooking.component.js -p && npm run build:utils && npm run build:mixin && npm run build:locale && npm run build:directives"
    },
    "dependencies": {
        "atom-ripple": "^0.0.2",
